{
  "private": true,
  "name": "storybook-chrome-screenshot-with-vue",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "corebuild": "cd ../../ && npm run build",
    "storybook": "start-storybook -p 9008 -s public",
    "screenshot": "node ../../lib/cli.js -p 9008 -c .storybook",
    "postinstall": "rm -rf node_modules/@storybook/addons",
    "prestorybook": "npm run corebuild",
    "prescreenshot": "npm run corebuild"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-centered": "^5.0.5",
    "@storybook/addon-knobs": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addon-notes": "^5.0.5",
    "@storybook/addon-viewport": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/vue": "^5.0.5",
    "babel-loader": "8.0.4",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "vue-hot-reload-api": "^2.3.0",
    "vue-loader": "^14.2.2",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "3.11.0",
    "webpack-dev-server": "^3.1.1"
  }
}
